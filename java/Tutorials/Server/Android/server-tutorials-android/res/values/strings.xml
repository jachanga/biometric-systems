    <!-- Hints -->
    <string name="msg_hint_server_admin">Server: Server IP \n\n
        Port: Server port number\n\nCommands:\n
        start - Start Cluster\nstop (id) - Stop\n(wait until finished task in progress) server (id is 0) or node (id is above or equal 4)
        \nkill (id) - Instantly stop server (id is 0) or node (id is above or equal 4)\ninfo (info type="")     - Print info about cluster or nodes
        \ndbupdate - DB update\ndbchanged (id)      - Notify server of changed templates in DB
        \ndbflush - Flush the database\nstatus - Gets current server status</string>
    <string name="msg_hint_server_status">Server: Server IP\n\nPort: Server port number</string>
    <string name="msg_hint_send_task">"Server: Server IP \n\nPort: Server port number \n\nTemplate: Template file name.
        \nif template file is not a NTemplate, Template type should be selected\n
        \nQuery: Specify the query. If query is empty, default query is used"</string>
    <string name="msg_hint_server_database">Server: Server IP\n\nPort:	Server port number\n\nTemplate:	Template file name.
